{
    "name": "rapidez/order-reminder",
    "description": "This package provides functionality within Rapidez to create order reminders for customers, prompting them to reorder products after a specified period.",
    "keywords": [
        "rapidez",
        "order-reminder"
    ],
    "homepage": "https://github.com/rapidez/order-reminder",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Claudio Ferraro",
            "email": "claudio@justbetter.nl",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1|^8.2",
        "rapidez/core": "^2.0",
        "rapidez/account": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Rapidez\\OrderReminder\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rapidez\\OrderReminder\\OrderReminderServiceProvider"
            ]
        }
    }
}
